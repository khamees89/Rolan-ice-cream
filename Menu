        ];

        // App state
        let cart = JSON.parse(localStorage.getItem('rolanCart')) || [];
        let currentCategory = 'all';
        let userPoints = parseInt(localStorage.getItem('rolanPoints')) || 250;
        let totalOrders = parseInt(localStorage.getItem('rolanOrders')) || 12;
        let savedAmount = parseInt(localStorage.getItem('rolanSaved')) || 12;

        // DOM elements
        const productsGrid = document.getElementById('productsGrid');
        const cartModal = document.getElementById('cartModal');
        const profileModal = document.getElementById('profileModal');
        const successModal = document.getElementById('successModal');
        const cartCount = document.getElementById('cartCount');
        const cartItems = document.getElementById('cartItems');
        const cartTotal = document.getElementById('cartTotal');

        // Save to localStorage
        function saveData() {
            localStorage.setItem('rolanCart', JSON.stringify(cart));
            localStorage.setItem('rolanPoints', userPoints.toString());
            localStorage.setItem('rolanOrders', totalOrders.toString());
            localStorage.setItem('rolanSaved', savedAmount.toString());
        }

        // Initialize app
        function init() {
            renderProducts();
            updateCartUI();
            updatePointsDisplay();
        }

        // Render products
        function renderProducts() {
            const filteredProducts = currentCategory === 'all' 
                ? products 
                : products.filter(p => p.category === currentCategory);

            productsGrid.innerHTML = filteredProducts.map(product => `
                <div class="card-hover bg-white rounded-xl shadow-lg overflow-hidden">
                    <div class="p-6">
                        <div class="text-center mb-4 product-image">
                            ${typeof product.image === 'string' && product.image.startsWith('<svg') 
                                ? product.image 
                                : `<div class="text-6xl">${product.image}</div>`
                            }
                        </div>
                        <h4 class="text-lg font-semibold mb-2 text-gray-800">${product.name}</h4>
                        <p class="text-gray-600 text-sm mb-4">${product.description}</p>
                        <div class="flex justify-between items-center mb-4">
                            <span class="text-2xl font-bold text-pink-600">${product.price} Ø¯.Ø£</span>
                            <span class="text-sm text-green-600 bg-green-100 px-2 py-1 rounded-full">+${product.points} Ù†Ù‚Ø·Ø©</span>
                        </div>
                        <button onclick="addToCart(${product.id})" class="w-full gradient-bg text-white py-2 rounded-lg font-semibold hover:opacity-90 transition-opacity">
                            Ø¥Ø¶Ø§ÙØ© Ù„Ù„Ø³Ù„Ø©
                        </button>
                    </div>
                </div>
            `).join('');
        }

        // Add to cart
        function addToCart(productId) {
            const product = products.find(p => p.id === productId);
            const existingItem = cart.find(item => item.id === productId);
            
            if (existingItem) {
                existingItem.quantity += 1;
            } else {
                cart.push({ ...product, quantity: 1 });
            }
            
            updateCartUI();
            saveData();
            
            // Animate cart badge
            cartCount.classList.add('cart-badge');
            setTimeout(() => cartCount.classList.remove('cart-badge'), 500);
        }

        // Update cart UI
        function updateCartUI() {
            cartCount.textContent = cart.reduce((sum, item) => sum + item.quantity, 0);
            
            cartItems.innerHTML = cart.map(item => `
                <div class="flex justify-between items-center bg-gray-50 rounded-lg p-3">
                    <div class="flex items-center space-x-3 space-x-reverse">
                        <span class="text-2xl">${typeof item.image === 'string' && item.image.startsWith('<svg') ? 'ğŸ¥ª' : item.image}</span>
                        <div>
                            <h5 class="font-semibold text-sm">${item.name}</h5>
                            <p class="text-xs text-gray-600">${item.price} Ø¯.Ø£ Ã— ${item.quantity}</p>
                        </div>
                    </div>
                    <div class="flex items-center space-x-2 space-x-reverse">
                        <button onclick="updateQuantity(${item.id}, -1)" class="w-6 h-6 bg-red-500 text-white rounded-full text-xs">-</button>
                        <span class="text-sm font-semibold">${item.quantity}</span>
                        <button onclick="updateQuantity(${item.id}, 1)" class="w-6 h-6 bg-green-500 text-white rounded-full text-xs">+</button>
                    </div>
                </div>
            `).join('');
            
            const total = cart.reduce((sum, item) => sum + (item.price * item.quantity), 0);
            cartTotal.textContent = `${total.toFixed(2)} Ø¯.Ø£`;
        }

        // Update quantity
        function updateQuantity(productId, change) {
            const item = cart.find(item => item.id === productId);
            if (item) {
                item.quantity += change;
                if (item.quantity <= 0) {
                    cart = cart.filter(cartItem => cartItem.id !== productId);
                }
                updateCartUI();
                saveData();
            }
        }

        // Update points display
        function updatePointsDisplay() {
            document.getElementById('userPoints').textContent = userPoints;
            document.getElementById('profilePoints').textContent = userPoints;
            document.getElementById('totalOrders').textContent = totalOrders;
            document.getElementById('savedAmount').textContent = `${savedAmount} Ø¯.Ø£`;
        }

        // Category filter
        document.querySelectorAll('.category-btn').forEach(btn => {
            btn.addEventListener('click', () => {
                document.querySelectorAll('.category-btn').forEach(b => {
                    b.classList.remove('active', 'bg-pink-500', 'text-white');
                    b.classList.add('bg-gray-200', 'text-gray-700');
                });
                
                btn.classList.add('active', 'bg-pink-500', 'text-white');
                btn.classList.remove('bg-gray-200', 'text-gray-700');
                
                currentCategory = btn.dataset.category;
                renderProducts();
            });
        });

        // Modal controls
        document.getElementById('cartBtn').addEventListener('click', () => {
            cartModal.classList.remove('hidden');
        });

        document.getElementById('closeCart').addEventListener('click', () => {
            cartModal.classList.add('hidden');
        });

        document.getElementById('profileBtn').addEventListener('click', () => {
            profileModal.classList.remove('hidden');
        });

        document.getElementById('closeProfile').addEventListener('click', () => {
            profileModal.classList.add('hidden');
        });

        document.getElementById('closeSuccess').addEventListener('click', () => {
            successModal.classList.add('hidden');
        });

        // Checkout
        document.getElementById('checkoutBtn').addEventListener('click', () => {
            if (cart.length === 0) {
                alert('Ø§Ù„Ø³Ù„Ø© ÙØ§Ø±ØºØ©!');
                return;
            }
            
            // Calculate earned points
            const earnedPoints = cart.reduce((sum, item) => sum + (item.points * item.quantity), 0);
            userPoints += earnedPoints;
            totalOrders += 1;
            
            // Generate order number
            const orderNumber = '#' + Math.floor(Math.random() * 9000 + 1000);
            
            // Update UI
            document.getElementById('orderNumber').textContent = orderNumber;
            document.getElementById('earnedPoints').textContent = earnedPoints;
            
            // Clear cart
            cart = [];
            updateCartUI();
            updatePointsDisplay();
            saveData();
            
            // Show success modal
            cartModal.classList.add('hidden');
            successModal.classList.remove('hidden');
        });

        // Close modals when clicking outside
        [cartModal, profileModal, successModal].forEach(modal => {
            modal.addEventListener('click', (e) => {
                if (e.target === modal) {
                    modal.classList.add('hidden');
                }
            });
        });

        // Initialize app
        init();
    </script>
